{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "gtkBuild",
            "command": "LSAN_OPTIONS=verbosity=1:log_threads=1;/usr/bin/gcc",
            "args": [
                "-fdiagnostics-color=always",
                "-g",
                "-fsanitize=address",
                "-Wall",
                "-Werror",
                /*"${workspaceFolder}/main.c",*/
                "${workspaceFolder}/converter.c",
                "${workspaceFolder}/string.c",
                "${workspaceFolder}/link_details.c",
                "$(pkg-config",
                "--cflags",
                "gtk4)",
                "-o",
                "${workspaceFolder}/out",
                "$(pkg-config",
                "--libs",
                "gtk4)",
                "-mfpmath=387"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        }
    ],
    "version": "2.0.0"
}